# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Configuration (for Better Auth)
DATABASE_URL=postgresql://postgres:your_password@db.your_project.supabase.co:5432/postgres

# Better Auth OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Better Auth Server Configuration
AUTH_SERVER_PORT=3001
BETTER_AUTH_SECRET=your_random_secret_key_here

# AI/ML Service Configuration (Optional - for resume parsing and job matching)
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_GEMINI_API_KEY=your_gemini_api_key

# External Job APIs (Optional - for job data integration)
VITE_INDEED_API_KEY=your_indeed_api_key
VITE_LINKEDIN_API_KEY=your_linkedin_api_key

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=your_ga_tracking_id

# Email Service (Optional - for notifications)
SUPABASE_SMTP_HOST=your_smtp_host
SUPABASE_SMTP_PORT=587
SUPABASE_SMTP_USER=your_smtp_username
SUPABASE_SMTP_PASS=your_smtp_password

# Development Configuration
NODE_ENV=development
VITE_APP_URL=http://localhost:3000

